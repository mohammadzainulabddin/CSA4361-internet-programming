    <title>Hospital Appointment Booking - Appointments Statistics</title>
</head>
<body>
    <header>
        <h1>Hospital Appointment Booking</h1>
    </header>
    <section>
        <h2>Appointments Statistics</h2>
        <!-- Display statistics related to appointments -->
        <p>Total Appointments: <span id="totalAppointments">100</span></p>
        <p>Completed Appointments: <span id="completedAppointments">80</span></p>
        <p>Upcoming Appointments: <span id="upcomingAppointments">20</span></p>
    </section>

    <script>
        // Fetch and update statistics using JavaScript or fetch data from the server
        document.getElementById('totalAppointments').innerText = getTotalAppointments();
        document.getElementById('completedAppointments').innerText = getCompletedAppointments();
        document.getElementById('upcomingAppointments').innerText = getUpcomingAppointments();

        // Add functions to fetch data from the server or update statistics dynamically
        function getTotalAppointments() {
            // Implement logic to get total appointments
            return 100;
        }

        function getCompletedAppointments() {
            // Implement logic to get completed appointments
            return 80;
        }

        function getUpcomingAppointments() {
            // Implement logic to get upcoming appointments
            return 20;
        }
    </script>
</body>
</html>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Hospital Appointment Booking - Rating Details</title>
</head>
<body>
    <header>
        <h1>Hospital Appointment Booking</h1>
    </header>
    <section>
        <h2>Rating Details</h2>
        <!-- Display details about ratings -->
        <ul>
            <!-- List ratings dynamically using JavaScript -->
            <li>
                <span>Doctor: Doctor Name</span>
                <span>Rating: 4.5</span>
                <span>Comments: Excellent service!</span>
            </li>
            <!-- ... -->
        </ul>
    </section>

    <script>
        // Add JavaScript functions for dynamic functionality
        // Fetch and update ratings from the server or fetch data dynamically
        document.addEventListener('DOMContentLoaded', function () {
            // Fetch and update ratings dynamically
            fetchRatings();
        });

        function fetchRatings() {
            // Implement logic to fetch ratings from the server or other data source
            const ratingsData = [
                { doctor: 'Doctor Name', rating: 4.5, comments: 'Excellent service!' },
                // ... add more ratings
            ];

            // Display ratings in the HTML
            const ratingsList = document.querySelector('ul');
            ratingsData.forEach(rating => {
                const li = document.createElement('li');
                li.innerHTML = `<span>Doctor: ${rating.doctor}</span><span>Rating: ${rating.rating}</span><span>Comments: ${rating.comments}</span>`;
                ratingsList.appendChild(li);
            });
        }
    </script>
</body>
</html>
